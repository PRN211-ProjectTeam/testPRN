@page
@model PetShop_Project_SWP391.Pages.Manager.Shippers.DetailModel
@{
    ViewData["Title"] = "Detail";
}
<link href="~/css/managerShipperdetail.css" rel="stylesheet" />

    <div class="manager-Shipper-detail container">
    <div class="row m-4 justify-content-between">
        <h1 class="col-6">Shipper Detail</h1>
        <div class="col-2">
            <a class="btn btn-primary" asp-page="./Index">Quay lại</a>
        </div>
        
     </div>
    <div class="mt-4">
        <dl class="row" style="padding-left : 10%">
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Shipper.Name)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model=>model.Shipper.Name)
            </dd>
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Shipper.Phone)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model=>model.Shipper.Phone)
            </dd>
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Shipper.Address)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model=>model.Shipper.Address)
            </dd>
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Shipper.District)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model=>model.Shipper.District)
            </dd>
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Shipper.Wards)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model=>model.Shipper.Wards)
            </dd>
            <dt class="col-sm-4">
                @Html.DisplayNameFor(model => model.Shipper.Province)
            </dt>
            <dd class="col-sm-8">
                @Html.DisplayFor(model=>model.Shipper.Province)
            </dd>
            
        </dl>
    </div>
</div>
